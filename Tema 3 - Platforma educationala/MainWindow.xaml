        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tema_3___Platforma_educationala"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <Window.Background>
        <ImageBrush ImageSource="/Tema 3 - Platforma educationala;component/Images/educatie.png"/>
    </Window.Background>

    <StackPanel>
        <Label Name="Username" Content="DigitalEDU" FontSize="30" Height="70" FontWeight="Bold" FontStyle="Oblique"/>
        <TextBox Height="36" Margin="0,0,549,0"/>
        <Button Content="Admin" Height="33" Margin="49,0,614,0" Click="Button_Admin"/>
        <Button Content="Profesor" Height="33" Margin="49,0,614,0" Click="Button_Profesor"/>
        <Button Content="Elev" Height="33" Margin="49,0,614,0" Click="Button_Elev"/>
    </StackPanel>
    
</Window>
